{
  "name": "nhl-stat-ingestor",
  "version": "0.0.1",
  "description": "This app is a self contained node application that ingests data from th NHL Stats API and stores it in a local sqllite db.",
  "type": "commonjs",
  "license": "MIT",
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/axios": "^0.14.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.11.10",
    "@types/uuid": "^9.0.1",
    "async-retry": "^1.3.3",
    "jest": "^29.5.0",
    "prisma": "^4.11.0",
    "ts-jest": "^29.0.5",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@prisma/client": "4.11.0",
    "axios": "^1.3.4",
    "better-sqlite3": "^8.2.0",
    "dotenv-cli": "^7.1.0",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev src/index.ts --watch",
    "test": " dotenv -e .env.test jest -i --silent",
    "test:loud": "dotenv -e .env.test jest -i",
    "test:e2e": "yarn test --testMatch='**/tests/e2e/**/?(*.)+(spec|test).ts' --silent",
    "test:unit": "yarn test --testMatch='**/tests/unit/**/?(*.)+(spec|test).ts' --silent",
    "setup": "sh bin/setup.sh"
  }
}
